library(ggplot2)
library(gifski)
library(gganimate)
# Generate data for the upward exponential curve
x <- seq(1, 40, length.out = 100)
y <- 5 * exp(0.1 * (x - 40)) # Exponential function starting low and increasing
data <- data.frame(x = x, y = y, frame = seq_along(x)) # Add a frame variable for animation
# Create the plot
p <- ggplot(data, aes(x = x, y = y)) +
geom_line(color = "white", size = 1) +  # Exponential curve
geom_point(aes(frame = frame), shape = 8, color = "#1E90FF", size = 4) + # Moving star
annotate("text", x = min(data$x), y = max(data$y), label = "ARC Lab", color = "white", size = 6, hjust = 0, vjust = 1) +
theme_void() + # Remove background, gridlines, and axes
theme(plot.background = element_rect(fill = "black", color = NA)) + # Black background
labs(title = "Exponential Function with Moving Star")
# Animate the plot
anim <- p + transition_reveal(frame)
# Save the animation as a GIF
anim_save("/Users/jtostlebe/Library/CloudStorage/OneDrive-UniversityofNebraskaatOmaha/ARC Lab/Graphics", anim, renderer = gifski_renderer(), width = 800, height = 400, res = 150)
